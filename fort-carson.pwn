    else if(kaina < 1) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Maþiausia kaina 1 ");
    else if(zInfo[id][Transportas1] !=-1) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas jau turi maðinà!");
    else if(GetPlayerScore(id) < 1500) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas per maþai patyræs!");
    else
    {
        SetPVarInt(id, "pardavejoID" , playerid);
        SetPVarInt(id, "mPrice" ,kaina);
        format(masyvas, sizeof(masyvas), "%s siûlo jums pirkti pirmà tr.priemonæ uþ %d ",GetPlayerNameEx(playerid), kaina);
        ShowPlayerDialogEx(id, DIALOG_MASINOS_PARDAVIMASZ, DIALOG_STYLE_MSGBOX, "Pardavimas",masyvas, "Pirkti", "Nepirkti");
    }
    return true;
}
