    if(zInfo[playerid][specialybe] == 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs neturite darbo!");
    if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs ne maðinoje!");
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Tik vairuotjas gali sunaikinti maðinà.");
    new vehicleid = GetPlayerVehicleID(playerid);
    //new model = GetVehicleModel(vehicleid);

    if (vInd[vehicleid][vehType] != VT_JOB) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Ði komanda tik darbinëms maðinoms");
    if (vInd[vehicleid][vehOwner] != zInfo[playerid][specialybe]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Sunaikinti galima tik savo darbo maðinas");

    SetVehicleToRespawn(vehicleid);

    switch (zInfo[playerid][specialybe])
    {
        case 1, 2, 15: DestroyObject(vehicleInfo[vehicleid][attachObject]);
    }

    DisablePlayerCheckpoint(playerid);
    for(new krov = 0; krov < 7; krov++) PlayerTextDrawHide(playerid, TDEditor_Krovinys[playerid][krov]);
    return true;
}

