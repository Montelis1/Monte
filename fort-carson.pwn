CMD:duoti(playerid,params[])
{
    new id, kiek,Float:poz[3],vardas[MAX_PLAYER_NAME],vardas2[MAX_PLAYER_NAME],masyvas[128];
    if(sscanf(params, "ud",id,kiek)) return SendClientMessage(playerid, BLUE, "{DFC043} {ffffff}Duoti þaidëjui pinigø: /duoti [Vardas_Pavarde] [Kiek]");
    else if(!IsPlayerConnected(id)) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Tokio þaidëjo nëra");
    else if(kiek < 1) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}0 pinigø duoti negalite!");
    else if(GetPlayerMoneyA(playerid) < kiek) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jus neturite tiek pinigø!");
    else if(GetPlayerScore(playerid) < 1500) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs per maþai patyræs!");
    else if(id == playerid) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Sau duoti pinigø negalite!");
    else if(IsPlayerNPC(id)) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Tokio þaidëjo nëra");
    else
    {
        GetPlayerName(playerid, vardas2, sizeof(vardas2));
        GetPlayerName(id, vardas, sizeof(vardas));
        GetPlayerPos(id, poz[0], poz[1], poz[2]);
        if(!PlayerToPoint(3, playerid, poz[0],poz[1],poz[2]))
        {
            SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas per toli...");
            return 1;
        }
        format(masyvas, sizeof(masyvas),"{ffffff}> {DFC043}Þaidëjas [{ffffff}%s{DFC043}] davë jums ({ffffff}%i{DFC043}) pinigø.",vardas2, kiek);
        SendClientMessage(id, BLUE,masyvas);
        format(masyvas, sizeof(masyvas),"{ffffff}> {DFC043}Davëte þaidëjui [{ffffff}%s{DFC043}] pinigø ({ffffff}%i{DFC043}).",vardas, kiek);
        SendClientMessage(playerid, BLUE, masyvas);
        GivePlayerMoneyA(id, kiek);
        GivePlayerMoneyA(playerid, -kiek);
        printf("Þaidëjas %s davë %s %i", vardas2, vardas, kiek);
    }
    return 1;
}
