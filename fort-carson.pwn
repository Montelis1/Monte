CMD:msiulyti(playerid, params[])
{
    if(zInfo[playerid][Transportas1] == -1) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs neturite tr.priemonës!");
    if(GetPlayerScore(playerid) < 1500) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs per maþai patyræs!");
    new id, kaina, masyvas[128];
    if(sscanf(params, "ud",id, kaina)) return SendClientMessage(playerid, BLUE, "{DFC043} {ffffff}Parduoti automobilá þaidëjui: /msiulyti [Vardas_Pavardë] [Kaina]");
