CMD:emigruoti(playerid, params[])
{
    if(!zInfo[playerid][aPriziuretojas] && zInfo[playerid][admin] < 4) return false;
    new id;
    if(sscanf(params, "ud",id)) return SendClientMessage(playerid, BLUE, "{DFC043} {ffffff}Emigruoti þaidëjà: /emigruoti [Vardas_Pavarde]");
    if(id == INVALID_PLAYER_ID) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Tokio þaidëjo nëra!");
    if(zInfo[id][Emigruotas]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas emigruotas!");
    //if(GetPlayerScore(id) > 5000) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas turi virð 5.000XP");
    SetPlayerScore(id, 1000 + GetPlayerScore(id));
    GivePlayerMoneyA(id, 10000);
    zInfo[id][Emigruotas] = true;
    zInfo[id][vip] = 1;
    zInfo[id][vplaikas] = gettime() + 864000;
    SendClientMessage(playerid, GREEN, "* Þaidëjas emigruotas!");
    SendClientMessage(id, GREEN, "* Buvote emigruotas!");
    return 1;
}
CMD:emigruotioff(playerid, params[])
