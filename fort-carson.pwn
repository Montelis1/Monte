stock AddJob(playerid, jText[], xp, license, medic, gangID, doc, invite, rank, jID)
{
    if(zInfo[playerid][specialybe]==jID) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Norint palikti darba /paliktidarba");
    if(zInfo[playerid][specialybe]!=0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jus jau turite darbà /paliktidarba!");
    if(GetPlayerScore(playerid) < xp) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jus nepakankamai patyræs!");
    if(!zInfo[playerid][dkvietimas][invite]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Direktorius apie jus neinformuotas");
    if(zInfo[playerid][KontraktoLaikas] >= gettime()) return SendClientMessage(playerid, RED,"Jûsø kontrakto laikas nepasibaigæs.");
    if(zInfo[playerid][protokolas] >= 3) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Turite protokolà, negalite ásidarbinti!");
    if(license == 1)
    {
       if(!zInfo[playerid][mteises]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jus neturite A kategorijos paþymëjimo!");
    }
    if(license == 2)
    {
       if(!zInfo[playerid][cteises]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jus neturite B kategorijos paþymëjimo!");
    }
    if(license == 3)
    {
       if(!zInfo[playerid][fteises]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jus neturite C kategorijos paþymëjimo!");
    }
    if(license == 4)
    {
       if(!zInfo[playerid][cteises]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jus neturite B kategorijos paþymëjimo!");
       if(!zInfo[playerid][pteises]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs neturite PPL kateg teisiø!");
    }
    if(medic != -1)
    {
       if(zInfo[playerid][medikupazyma] < gettime()) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Neturite sveikatos paþymëjimo!");
    }
    if(gangID != -1)
    {
       if(playerGang[playerid] != INVALID_GANG_ID) return ErorMSG(playerid, "Esate gaujoje todël á teisësauga eiti draudþiama!");
    }
    new str[200];
    format(str, 200, "{DFC043} ISIDARBINIMAS  {FFFFFF}Sveikiname, jûs priimtas á %s darbà.", jText);
    SendClientMessage(playerid, BLUE, str);
    SendClientMessage(playerid, BLUE, "{DFC043} ISIDARBINIMAS  {FFFFFF}Jûsø darbo komandos: /darbokomandos");
    PlayerPlaySound(playerid,1149,0.0,0.0,0.0 );
    zInfo[playerid][specialybe]=jID;
    zInfo[playerid][drabuziai]=GetPlayerSkin(playerid);
    zInfo[playerid][KontraktoLaikas]=gettime() + doc * Kontraktas[playerid];
    zInfo[playerid][skinStatus] = SKINSTATUS_JOB;
    zInfo[playerid][uniforma] = get_rangSkin (zInfo[playerid][specialybe], rank, zInfo[playerid][lytis]);
    SetPlayerSkin(playerid, zInfo[playerid][uniforma]);
    ApplyAnimation(playerid,"PLAYIDLES","stretch",4.1,0,0,0,0,0);
    zInfo[playerid][JobRank] = rank;
    /*new String[200];
    format(String,200,"{DFC043}[racija] %s: Ásidarbino á darbà!",GetPlayerNameEx(playerid));
    Zinuteswat(-1,String);*/
	return 1;
}
