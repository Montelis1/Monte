CMD:sms(playerid, params[])
{
    if(CheckMute(playerid)) return 0;
    if(zInfo[playerid][telefonas] == 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs neturite telefono!");
    if(zInfo[playerid][kortele] == 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs neturite kortelës!");
    if(zInfo[playerid][mobsas] < 2) return SendClientMessage(playerid,RED,"{DFC043} {ffffff}Jûs neturite sàskaitoje!");
    if(zInfo[playerid][telefonostat] == 0) return SendClientMessage(playerid,RED,"{DFC043} {ffffff}Jûsø telefonas iðjungtas!");
    new recieverid,text[128],vardas[MAX_PLAYER_NAME],vardas2[MAX_PLAYER_NAME];
    if(sscanf(params, "us[128]", recieverid, text)) return SendClientMessage(playerid,BLUE, "{DFC043} {ffffff}Siûsti þinutæ þaidëjui: /sms [Vardas_Pavarde] [Þinutë]");
    else if(recieverid == INVALID_PLAYER_ID) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Tokio þaidëjo nëra!");
    else if(zInfo[recieverid][telefonas] == 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas neturi telefono!");
    else if(zInfo[recieverid][kortele] == 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas neturi kortelës!");
    else if(zInfo[recieverid][mutelaikas] > 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas yra uþtildytas!");
    else if(zInfo[recieverid][AFK_Stat] || zInfo[recieverid][Ofisas]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas yra AFK bûsenoje, þinutë nebuvo nusiøsta!");
    else
    {
        GetPlayerName(playerid,vardas2,sizeof(vardas2));
        GetPlayerName(recieverid,vardas,sizeof(vardas));

        if(strfind(vardas, "TELE2", true) != -1)
        {
            SetTimerEx("SMSsystem",(1+random(4))*1000,false,"is",playerid,text);
            SendSMS(INVALID_PLAYER_ID, vardas, "TELE2", text, playerid);
            return 1;
        }

        if(recieverid == INVALID_PLAYER_ID) { SendClientMessage(playerid, RED, "Tokio vardo nera"); return 1; }
        if(!zInfo[recieverid][telefonostat])
        {
            SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjo telefonas iðjungtas!");
            return 1;
        }
        zInfo[playerid][mobsas]-=2;
        SendSMS(recieverid,vardas2,vardas,text,playerid);
    }
    return 1;
}
