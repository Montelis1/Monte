            medziojimoDuom[r][ stirnosid ] = CreateDynamicObject(medziojimoDuom[r][ modelioid ], medziojimoDuom[r][ posX ], medziojimoDuom[r][ posY ], medziojimoDuom[r][ posZ ], 0.00000, 0.00000, 0.00000);
            format(medziojimoDuom[r][zudikas], 24, "zudikas");
            medziojimoDuom[r][respawn] = 0;
            medziojimoDuom[r][status] = 1;
        }
        else if(medziojimoDuom[r][status] == 2)
        {
            medziojimoDuom[r][respawn]++;
            if(medziojimoDuom[r][respawn] >=2)
            {
                DestroyDynamicObject(medziojimoDuom[r][stirnosid]);
                Delete3DTextLabel(medziojimoDuom[r][label]);
                medziojimoDuom[r][kapojimoprog] = 0;
                medziojimoDuom[r][status] = 3;
            }
        }
    }


    return 1;
}

