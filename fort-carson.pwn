CMD:pagydyt(playerid, params[])
{
    if(zInfo[playerid][ikalintas]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs kalëjime!");
    if(BankoBomba == 1 && IsPlayerInRangeOfPoint(playerid, 150, -451.4993,-1246.2987,2999.8909)) return SendClientMessage(playerid, -1, "Vyksta banko grobimas todël komandos naudoti negalite!");
    if(NarkuBomba == 1 && IsPlayerInRangeOfPoint(playerid, 150, 262.9246,228.1310,1007.6704)) return SendClientMessage(playerid, -1, "Vyksta narkotikø saugyklos grobimas todël komandos naudoti negalite!");
    if(zInfo[playerid][admin] < 2 && !zInfo[playerid][aPriziuretojas] && !zInfo[playerid][ePriziuretojas] &&  !zInfo[playerid][vPriziuretojas] && !zInfo[playerid][dPriziuretojas] && !zInfo[playerid][bPriziuretojas]) return 0;
    if(Pazymetas[playerid] == 1) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs esate paþymëtas bëgliu!");
    if (turiMafPack[playerid]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Su paketu negalima");
    new id, vardas[MAX_PLAYER_NAME], vardas2[MAX_PLAYER_NAME], masyvas[200], Float:vPos[3], string[200];
    if(sscanf(params, "u",id)) return SendClientMessage(playerid, BLUE, "{DFC043} {ffffff}Pagydyti þaidëja: /pagydyt [Vardas_Pavarde]");
    if(id == INVALID_PLAYER_ID) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Tokio þaidëjo nëra!");
    if(id == playerid) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Savæs gydyti negalite!");
    if (turiMafPack[id]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas su paketu");

    GetPlayerName(id, vardas, sizeof(vardas));
    GetPlayerName(playerid, vardas2, sizeof(vardas2));
    format(masyvas, sizeof(masyvas), "{DFC043} {ffffff}Jûs suteikëte þaidëjui {DFC043}%s {FFFFFF}gyvybiø.",vardas);
    SendClientMessage(playerid, GREEN, masyvas);
    format(masyvas, sizeof(masyvas), "{DFC043} {ffffff}Administratorius{DFC043} %s {FFFFFF}suteikë jums gyvybiø!",vardas2);
    SendClientMessage(id, GREEN, masyvas);
    SetPlayerHealth(id, 99);
    SetPlayerArmour(id, 99);
    GetPlayerPos(playerid, vPos[0], vPos[1], vPos[2]);
    format(string, 200, "{DFC043} {ffffff}Administratorius {DFC043}%s {FFFFFF}suteikë gyvybiø {DFC043}%s {FFFFFF}!",vardas2, vardas);

    for(new p = 0; p < MAX_PLAYERS; p++)
    {
        if(IsPlayerConnected(p))
        {
            if(IsPlayerInRangeOfPoint(p, 15.0, vPos[0], vPos[1], vPos[2]))
            {
                SendClientMessage(p, RED, string);
            }
        }
    }

    if(!zInfo[playerid][NewPasiekimai][1])
    {
       zInfo[playerid][NewPasiekimai][1] = 1;
       zInfo[playerid][paskutpas] = 12;
       new query[200];
       format(query, sizeof(query), "UPDATE zaidejai SET NewPasiekimai2 = '1' WHERE vardas = '%s'",GetPlayerNameEx(playerid));
       mysql_query(query);
       SendClientMessage(playerid, -1, "{DFC043} {ffffff}Ávygdei pagydyti þaidëjà pasiekima, gavai {DFC043}5 {FFFFFF}kreditø, {DFC043}100XP {FFFFFF}ir {DFC043}1500!");
       zInfo[playerid][kreditai]+=5;
       SetPlayerScore(playerid, GetPlayerScore(playerid) + 100);
       GivePlayerMoneyA(playerid,1500);
    }
    return 1;
}
