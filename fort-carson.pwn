    CRcar3D[carCP[playerid]] = Create3DTextLabel(String, -1, -1102.7327, -1623.6302, 76.2715, 50.0, 0, 0);
    Attach3DTextLabelToVehicle(CRcar3D[carCP[playerid]], carCP[playerid], 0.0, 0.0, 1.0);
	SendClientMessage(playerid, -1, "{FFFFFF}Transportas {DFC043}EVENTAMS {FFFFFF}sukurtas");
	SetPlayerHealth(playerid, 100);
	SetPlayerArmour(playerid, 99);
	return 1;
}
