CMD:mapp(playerid,params[])
{
    new name[MAX_PLAYER_NAME];
    new msg[180],Float:coo[6],id;
    if(sscanf(params, "u",id)) return SendClientMessage(playerid, BLUE, "{DFC043} {ffffff}Þaidëjo pozicija: /mapp [Vardas_Pavarde]");
    if(zInfo[id][admin] == 5) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Pas savininkø pozicijos nustatyti negalite!");
    else if(id == INVALID_PLAYER_ID) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Tokio þaidëjo nëra");
    else if(id == playerid) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Savo pozicija þinai!");
    else if(IsPlayerNPC(id)) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Tokio þaidëjo nëra");
    else if(GetPlayerInterior(id) > 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas yra interjere!");
    else
    {
        GetPlayerName(id,name,sizeof(name));
        GetPlayerPos(id,coo[0],coo[1],coo[2]);
        GetPlayerPos(playerid,coo[3],coo[4],coo[5]);
        new Float:atstumas = ((floatabs(coo[0]-coo[3])+floatabs(coo[1]-coo[4])+floatabs(coo[2]-coo[5]))/3)/100;
        SetPlayerCheckpoint(playerid,coo[0],coo[1],coo[2],2);
        new salis1[10],salis2[10];
        if(coo[0]-coo[3]>0) salis1="rytus"; else salis1="vakarus";
        if(coo[1]-coo[4]>0) salis2="ðiaurës"; else salis2="pietø";
        format(msg,sizeof(msg),"{ffffff}> {99ff33}({ffffff}%s{99ff33}) lokacija rodoma mini þemëlapyje. Jis yra uþ {ffffff}%0.1fKM{99ff33} á %s %s nuo jûsø.",name,atstumas,salis2,salis1);
        SendClientMessage(playerid,Zalia,msg);
    }
    return 1;
}
