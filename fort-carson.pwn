stock ZinuteLaivams(color,text[])
{
    for(new i=0; i<MAX_PLAYERS; i++)
    {
        new vehicleid = GetPlayerVehicleID(i);
        if(IsPlayerConnected(i))
        {
            if(GetVehicleModel(vehicleid) == 453 && lInfo[vehicleid][Racija] == 1)
            {
                SendClientMessage(i,color,text);
            }
        }
    }
}
forward EveryMinute();
public EveryMinute()
{
    new Hours,Minutes;
    gettime(Hours, Minutes);
    /*if(Hours >= 18) //þiemos oras
    {
        SetWorldTime(22);
    }*/
    if(Hours > 7 && Hours < 22)
    {
        SetWorldTime(14); //vasaros oras
        //SetWorldTime(23); //þeimos oras
    }
    else if(Hours > 22 && Hours < 6)
    {
        SetWorldTime(23);
    }
   /* else if(Hours > 19 && Hours < 23)
    {
        DyleriaiGun += 120;
        MoveDynamicObject(gangGate[1], 997.12732, 2133.21655, 7.94400, 8, 0.00000, 0.00000, 90.09900);
        //SendClientMessageToAll(-1, "{878787} SANDËLIS  {FFFFFF}Serveryje atsidarë {878787}ginklø sandëlis {FFFFFF}gaujos skubëkite ten{878787}.");
	}
    else if(Hours > 23)
    {
        MoveDynamicObject(gangGate[1], 997.12732, 2133.21655, 11.55200, 10, 0.00000, 0.00000, 90.09900);
        //SendClientMessageToAll(-1, "{878787} SANDËLIS  {FFFFFF}Serveryje uþsidarë {878787}ginklø sandëlis {FFFFFF}nes jis veikia nuo 19:00 iki 22:00 valandos{878787}.");
	}*/

    gs_check_ship();

    new Year,Month,Day;
    getdate(Year, Month, Day);
    if(Year == 2018 && Month == 10 && Day == 5 && Hours == 19 && Minutes == 00)
    {
	   SendRconCommand("hostname fort-carson.lt - renginys mums jau metukai!");
       birtOn = 1;
    }
    else if(Year == 2018 && Month == 10 && Day == 6)
    {
	   SendRconCommand("hostname fort-carson.lt - miesto ir miestelio gyvenimas");
       birtOn = 0;
    }
}
forward SecondsTimer();
public SecondsTimer()
{
    new h,m,s;
    gettime(h, m,s);
    if(s == 0 && EveryMinuteTimerCreated == false)
    {
        EveryMinuteTimerCreated = true;
        SetTimer("EveryMinute", 60000, true);
    }

    check_grobPD();
    //check_grobMK ();
    check_hp();
}






