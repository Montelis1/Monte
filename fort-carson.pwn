CMD:addbiz(playerid, params[])
{
    if(zInfo[playerid][admin] < 5) return 0;
    new price, uzdar, Float:pPos[3], pav[50], npav[50], bQuery[45], msg[500];
    if(sscanf(params, "s[50]dd", pav, price, uzdar)) return SendClientMessage(playerid, -1, "Komandos naudojimas: /addbiz [pavadinimas] [kaina] [uzdrbis]");
    //else if(zInfo[playerid][admin] == 6) return SendClientMessage(playerid, RED, "Pagalbininkui ði komanda apribota!");
	format(npav, 50, "%s", DeleteAllLowLines(pav));
    for(new biz; biz < MAX_BUSSINESS; biz++)
    {
        format(bQuery, 45, "SELECT * FROM `bussiness` WHERE `ID` = '%d'", biz);
        mysql_query(bQuery);
        mysql_store_result();
        if(!mysql_num_rows())
        {
            bInfo[biz][bCreated] = true;
            bInfo[biz][Nupirktas] = false;
            format(bInfo[biz][Savininkas], MAX_PLAYER_NAME, "Nera");
            format(bInfo[biz][bPavadinimas], 50, "%s", npav);
            bInfo[biz][Pasaulis] = GetPlayerVirtualWorld(playerid);
            GetPlayerPos(playerid, pPos[0], pPos[1], pPos[2]);
            bInfo[biz][bpX] = pPos[0];
            bInfo[biz][bpY] = pPos[1];
            bInfo[biz][bpZ] = pPos[2];
            bInfo[biz][Kaina] = price;
            bInfo[biz][Uzdarbis] = uzdar;
            bInfo[biz][Uzdirbta] = 0;
            bInfo[biz][Mokesciai] = 0;
            bInfo[biz][b_alarm] = BUSSINESS_ALARM_NONE;
            format(msg, 500, "{ffffff}Ðis verslas yra parduodamas serverio !\n\n{ffffff}Verslo pavadinimas {DFC043}%s\n{ffffff}Verslo numeris: {DFC043}%d\n{ffffff}Verslo kaina {DFC043}%d eurai\n\n{ffffff}Norëdami ásigyti ðá verslà spauskite {DFC043}ENTER", bInfo[biz][bPavadinimas], biz, bInfo[biz][Kaina]);
            bInfo[biz][Bussiness3D] = Create3DTextLabel(msg, -1, bInfo[biz][bpX], bInfo[biz][bpY], bInfo[biz][bpZ], 10, bInfo[biz][Pasaulis], 0);
            bInfo[biz][BussinessPickup] = CreatePickup(1212, 1, bInfo[biz][bpX], bInfo[biz][bpY], bInfo[biz][bpZ], bInfo[biz][Pasaulis]);
            format(bQuery, 45, "INSERT INTO `bussiness` (ID) VALUES ('%d')", biz);
            mysql_query(bQuery);
            format(msg, 81, "verslas pavadinimu %s, sukurtas.", npav);
            SendClientMessage(playerid, -1, msg);
            SaveBussines(biz);
            return 1;
        }
        mysql_free_result();
    }
    return SendClientMessage(playerid, -1, "{DFC043} {ffffff}Daugiau verslø sukurti neimanoma.");
}
