    format(uQuerry, 150, "SELECT * FROM `zaidejai` WHERE `vardas` = '%s'", zzVardas);
    mysql_query(uQuerry);
    mysql_store_result();
    mysql_retrieve_row();
    if(mysql_num_rows())
