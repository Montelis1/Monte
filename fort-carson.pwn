{
	new skaic, o = GetMaxPlayers();
	for(new m; m < o; m++) if(IsPlayerConnected(m) && !IsPlayerNPC(m)) skaic ++;
	return skaic;
}

GetWeekDay(day=0, month=0, year=0)
{
  if (!day)
    getdate(year, month, day);

  new
    j,
    e
  ;

  if (month <= 2)
  {
    month += 12;
    --year;
  }

  j = year % 100;
  e = year / 100;

  return ((day + (month+1)*26/10 + j + j/4 + e/4 - 2*e) % 7);
}


