    else if(zInfo[id][admin] == 5) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Savininko surakinti negalite!");
	else if(zInfo[id][specialybe] == 13) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas dirba jûsø darbe!");
