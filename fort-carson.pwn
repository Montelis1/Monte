CMD:naudoti(playerid)
{
    if(zInfo[playerid][KadaVartojo] > gettime()) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Narkotikus vartoti galima tik kas minute.!");
    if(Tazed[playerid]) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs esate paðautas tazerio!");
    if(zInfo[playerid][kokainas] < 1) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs neturite kokaino!");
    ApplyAnimation(playerid, "PED", "WALK_DRUNK", 4.1, 1, 1, 1, 1, 1);
    zInfo[playerid][kokainas] -= 1;
    SetPlayerArmour(playerid, 99);
    SendClientMessage(playerid, GREEN, "* Sunaudojote kokainà!");
    if(zInfo[playerid][PriklausBalai] != 100)
    {
        zInfo[playerid][PriklausBalai]+=5;
    }
    if(zInfo[playerid][PriklausBalai] == 100)
    {
        zInfo[playerid][Priklausomybe] = true;
        Priklausomas[playerid] = -10;
    }
    new Float:vPos[3];
    GetPlayerPos(playerid, vPos[0], vPos[1], vPos[2]);
    for(new p = 0; p < MAX_PLAYERS; p++)
    {
        if(IsPlayerConnected(p))
        {
            if(IsPlayerInRangeOfPoint(p, 15.0, vPos[0], vPos[1], vPos[2]))
            {
                new Kiekis[120];
                format(Kiekis, sizeof Kiekis, "Þaidëjas %s sutraukë kokainà per nosá.",GetPlayerNameEx(playerid));
                SendClientMessage(p, BLUE, Kiekis);
            }
        }
    }
    zInfo[playerid][KadaVartojo] = gettime() + 60;
    return 1;
}
