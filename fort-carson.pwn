CMD:bomba(playerid)
{
    if (playerGang[playerid] == INVALID_GANG_ID)
        return 0;

    new Float:Coo[3];
    GetPlayerPos(playerid,Coo[0],Coo[1],Coo[2]);
    if(IsPlayerInRangeOfPoint(playerid, 5.0, -407.0577,2716.4036,62.2765))
    {
        //if(zInfo[playerid][specialybe] != 15) return SendClientMessage(playerid, RED, "Bombà ant kartelio vartø gali dëti tik FTB!");
        if (strcmp(gangDB[playerGang[playerid]][gLeaderName], GetPlayerNameEx(playerid)) != 0) return 0;
        if(YakuzaBomba == 1) return SendClientMessage(playerid, -1, "Bomba jau padëta!");
        SendClientMessage(playerid, -1, "Padëjote bombà ant kartelio vartø! Ji sprogs po 30 sec.");
        SetTimer("BombaYakuza", 1*30000, false);

        ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.0, 0, 0, 0, 0, 0);
        for(new i=0;i<MAX_PLAYERS;i++)
        {
            if(IsPlayerConnected(i))
            {
                if(IsPlayerInRangeOfPoint(i, 10, Coo[0],Coo[1],Coo[2]))
                {
                    if(zInfo[i][gaujosID])
                    {
                        GameTextForPlayer(i, "~G~ Bomba padëta, ji sprogs uþ 30 sec. Slëpkites!", 3000, 5);
                        SendClientMessage(i, -1, "Bomba padëta, ji sprogs uþ 30 sec. Slëpkites!."); // nusiunciam zinute
                        //SetPlayerCheckpointEx(i, 0, -215.5425,2725.6416,62.7028, 3.0);
                    }
                }
            }
        }

        YakuzaBomba = 1;
        return 1;
    }
    if(IsPlayerInRangeOfPoint(playerid, 2.0, -215.5425,2725.6416,62.7028))
    {
        if(zInfo[playerid][specialybe] != 15) return SendClientMessage(playerid, RED, "Bombà ant kartelio durø gali dëti tik FTB!");
        if(DyluBomba == 1) return SendClientMessage(playerid, -1, "Bomba jau padëta!");
        SendClientMessage(playerid, -1, "Padëjote bombà ant kartelio durø! Ji sprogs po 30 sec.");
        SetTimer("BombaDylai", 1*30000, false);

        Zinuteswat(-1,"{ecf0f1}[Policijos racija]:{95a5a6} Dëmesio! Bomba padëta ant kartelio durø, ji sprogs po 30 sec.");
        Zinuteswat(-1,"{ecf0f1}[Policijos racija]:{95a5a6} Dëmesio! Reidas truks 5 min. po bombos sprogimo!");
