CMD:premijos(playerid, params[])
{
    if(zInfo[playerid][dyrikz]==0) return 0;
    new DarbKiekis, algosk;
    new kiekis, darbs;
    if(zInfo[playerid][ikalintas]) return SendClientMessage(playerid, RED, "* Jûs kalëjime!");
    if(sscanf(params, "d",kiekis)) return SendClientMessage(playerid, BLUE, "* Iðdalinti premijas netoli esantiems darbuotojams: /premijos [Pinigø suma]");
    if(kiekis > Pelnas[GetPelnasIDFromLeader(playerid)]) return SendClientMessage(playerid, RED, "* Pelne tiek pinigø nëra!");
    if(kiekis <= 1000 || kiekis > 25000 ) return SendClientMessage(playerid, RED, "* Pinigø suma turi nuo 1000 Eur iki 25.000 Eur!");
    if(DarbKiekis == 0 ) return SendClientMessage(playerid, RED, "* Serveryje nëra jûsø darbuotojø!");
    new masyvas[150], Float:vPos[3];
    GetPlayerPos(playerid, vPos[0], vPos[1], vPos[2]);
    Pelnas[GetPelnasIDFromLeader(playerid)] -= kiekis;
    format(masyvas, sizeof(masyvas), "{ffffff}>{0066ff} Iðdalinote premijas ðalia esantiems darbuotojams ({ffffff}Viso iðdalinta: %d{0066ff})!", kiekis);
    SendClientMessage(playerid, GREEN, masyvas);
    if(zInfo[playerid][dyrikz] > 0) darbs = GautiDarba(zInfo[playerid][dyrikz]);
    else darbs = GautiDarba(zInfo[playerid][pavaz]);


    for(new p = 0; p < MAX_PLAYERS; p++)
    {
        if(IsPlayerConnected(p))
        {
            if(IsPlayerInRangeOfPoint(p, 20.0, vPos[0], vPos[1], vPos[2]))
            {
                if(zInfo[p][specialybe] == darbs && p != playerid)
                {
                    DarbKiekis ++;
                }
            }
        }
    }
    algosk = kiekis/DarbKiekis;
    for(new p = 0; p < MAX_PLAYERS; p++)
    {
        if(IsPlayerConnected(p))
        {
            if(IsPlayerInRangeOfPoint(p, 20.0, vPos[0], vPos[1], vPos[2]))
            {
                if(zInfo[p][specialybe] == darbs && p != playerid)
                {
                    format(masyvas, sizeof(masyvas), "{ffffff}> {0066ff} Gavote ({ffffff}%d{0066ff}) eurø premijà ið direktoriaus!", algosk);
                    SendClientMessage(p, GREEN, masyvas);
                    GivePlayerMoneyA(p, algosk);
                }
            }
        }
    }
    return 1;
}

