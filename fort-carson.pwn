CMD:prespawn(playerid)
{
    if(zInfo[playerid][pavaz]==0) return 0;
    SendClientMessage(playerid, GREEN, "* Darbo transportas atnaujintas!");

    for (new i; i < MAX_VEHICLES; i++)
    {
        if (vInd[i][vehType] != VT_JOB) continue;
        if (vInd[i][vehOwner] == zInfo[playerid][pavaz])
        {
            if (!VehicleHasDriver(i)) SetVehicleToRespawn(i);
        }
    }

    return 1;
}

