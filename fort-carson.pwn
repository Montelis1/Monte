resetVarzybos()
{
    new x, str[200], query[300];
    mysql_query("SELECT * FROM zaidejai WHERE Admin!='5' ORDER BY Prazaide DESC LIMIT 1");
    mysql_store_result();
    while(mysql_retrieve_row())
    {
        new KoksVardas[24], KiekLt[10];
        x ++;
        mysql_fetch_field_row(KoksVardas, "vardas");
        mysql_fetch_field_row(KiekLt, "Prazaide");
        format(str, 200, "{DFC043} {ffffff}Ðios dienos aktyvumo varþybø pirmà vietà laimëjo {FFEF81}%s{FFE333}.", KoksVardas);
        SendClientMessageToAll(-1,str);
        format(query, sizeof(query), "UPDATE zaidejai SET LaimejoVar = '1' WHERE vardas = '%s'",KoksVardas);
        mysql_query(query);
        mysql_query("UPDATE `zaidejai` SET `prazaide` = '0'");
    }
    mysql_free_result();
    return 1;
}

