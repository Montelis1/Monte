	}
    //DestroyVehicle(carCP[playerid]);
	carCP[playerid] = AddVehicle(vehicleid, carX, carY, carZ+2.0, carA, spalva1, spalva2, 600, VT_CARCREATE, CAR_EVT);
	LinkVehicleToInterior(carCP[playerid], GetPlayerInterior(playerid));
	PutPlayerInVehicleEx(playerid, carCP[playerid], 0);
	tInfo[carCP[playerid]][Degalai]= 20;
    new String[100];
