CMD:pelnas(playerid)
{
    if(zInfo[playerid][specialybe] > 0 && zInfo[playerid][specialybe] < 17 && zInfo[playerid][dyrikz] == 0)
    {
        new str[30];
        format(str, sizeof(str), "{ffffff}Pelne: %d", Pelnas[GetPelnasIDFromJob(playerid)]);
        ShowPlayerDialogEx(playerid, DIALOG_NONE, DIALOG_STYLE_MSGBOX, "{ffffff}Darbo Pelnas", str, "Atgal", "");
        return 1;
    }
    if(zInfo[playerid][dyrikz] < 1) return false;
    if(GetPelnasIDFromLeader(playerid) == -1) return 1;
    ShowPlayerDialogEx(playerid, DIALOG_PELNAS, DIALOG_STYLE_LIST, "Pelno valdymas", "Balansas\nPadeti", "Rinktis", "Iðeiti");
    return 1;
}
