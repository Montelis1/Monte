CMD:tr(playerid, params[])
{
    new drb = zInfo[playerid][specialybe];
    if(!(drb == 1 || drb == 15 || drb == 16)) return SendClientMessage(playerid,Raudona,"Jûs neturite teisësaugos racijos.");
    if(CheckMute(playerid)) return 0;
    new tekstas[256],msg[300],name[MAX_PLAYER_NAME],Sritis[60];
    if(sscanf(params, "s[256]",tekstas)) return SendClientMessage(playerid, BLUE, "{DFC043} {ffffff}Kalbëti per teisësaugos racija: /tr [Tekstas]");
    else
    {
        GetPlayerName(playerid, name, sizeof(name));

        if(zInfo[playerid][specialybe] == 1) Sritis = "PD";//
        else if(zInfo[playerid][specialybe] == 15) Sritis = "FTB";//
		//else if(zInfo[playerid][specialybe] == 16) Sritis = "Kar";
        format(msg,sizeof(msg),"{DFC043}[teisësaugos racija] %s(%s):{ffcc00} %s",name,Sritis,tekstas);
        ZinuteTeisesauga(BLUE,msg);
    }
    return 1;
}

