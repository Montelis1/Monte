CMD:skamb(playerid, params[])
{
    if(zInfo[playerid][telefonas] == 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs neturite telefono!");
    if(zInfo[playerid][kortele] == 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs neturite kortelës!");
    if(zInfo[playerid][mobsas] < 3) return SendClientMessage(playerid,RED,"{DFC043} {ffffff}Skambuèiui neuþtenka pinigø!");
    new id;
    if(sscanf(params, "u",id)) return SendClientMessage(playerid, BLUE, "{DFC043} {ffffff}Skambinti þaidëjui: /skamb [Vardas_Pavarde]");
    else if(id == INVALID_PLAYER_ID) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Tokio þaidëjo nëra!");
    else if(id == playerid) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Sau skambinti negalite1");
    else if(zInfo[id][skambutis] !=-1) SendClientMessage(playerid, RED, "{DFC043} {ffffff}Operatorius: Linija uzimta.");
    else if(zInfo[id][telefonostat] == 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjo telefonas iðjungtas!");
    else if(zInfo[id][telefonas] == 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas neturi telefono!");
    else if(zInfo[id][kortele] == 0) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas neturi kortelës!");
    else
    {
        if (GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_NONE)
