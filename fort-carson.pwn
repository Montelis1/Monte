public OnPlayerGiveDamage(playerid, damagedid, Float:amount, weaponid)
{
    if(IsPlayerNPC(damagedid)) return 1;
    zInfo[damagedid][Nuskriaude] = playerid;
    if(zInfo[playerid][specialybe])
    {
        if(Tazer[playerid] && GetPlayerWeapon(playerid) == 23)
        {
            new Float:health,losehp,string[128];
            GetPlayerHealth(damagedid,health);
            losehp = TAZE_LOSEHP;
            new Float:x, Float:y, Float:z;
            GetPlayerPos(damagedid, x, y, z);
            ClearAnimations(damagedid);
            if(losehp) {} else SetPlayerHealth(damagedid, health+amount);
            if(!PlayerToPoint(20, playerid, x,y,z)) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Tazeris ið taip toli neveikia.");
            if(Tazed[damagedid]) return 1;
            ClearAnimations(damagedid);
            ApplyAnimation(damagedid,"PED","KO_skid_front",4.1,0,1,1,1,0);
            Spark[damagedid] = CreateObject(TAZE_SPARK, x, y, z-3, 0, 0, 0);
            SetTimerEx("DestroySpark", TAZE_DESTROY, 0, "i", damagedid);
            SetTimerEx("TazedRemove", TAZE_TIMER, 0, "i", damagedid);
            Tazed[damagedid] = true;
            if(losehp) {} else SetPlayerHealth(damagedid, health+amount);
            format(string, sizeof(string), "Paðovëte þaidëjà su tazeriu: %s.", GetPlayerNameEx(damagedid));
            SendClientMessage(playerid, RED, string);
            format(string, sizeof(string), "Buvote paðautas tazerio nuo: %s.", GetPlayerNameEx(playerid));
            SendClientMessage(damagedid, RED, string);
        }
    }
    return 1;
}
public OnPlayerWeaponShot( playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ )
