	if(zInfo[playerid][admin] < 3 && !zInfo[playerid][aPriziuretojas] && !zInfo[playerid][ePriziuretojas] && !zInfo[playerid][vPriziuretojas] && !zInfo[playerid][dPriziuretojas] && !zInfo[playerid][bPriziuretojas]) return 0;
	new vehicleid = GetPlayerVehicleID(playerid), spalva1, spalva2;
	new Float: carX, Float: carY, Float: carZ, Float: carA;
	GetPlayerPos(playerid, carX, carY, carZ);
	GetPlayerFacingAngle(playerid, carA);
	if(sscanf(params,"ddd", vehicleid, spalva1, spalva2)) return InfoMSG(playerid, "Komandos naudojimas: /kurticar [Maðinos ID] [Pirma spalva] [Antra spalva]");
