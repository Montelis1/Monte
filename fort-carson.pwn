CMD:dirbu(playerid, params[])
{

    if(zInfo[playerid][specialybe] != 1 && zInfo[playerid][specialybe] != 2 && zInfo[playerid][specialybe] != 5 && zInfo[playerid][specialybe] != 3 && zInfo[playerid][specialybe] != 6 && zInfo[playerid][specialybe] != 13) return SendClientMessage(playerid, RED, "Atsipraðome, jûs nedirbate darbo su pakvietimu !");
    if(zInfo[playerid][DirbuEme] > gettime()) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Ðità komanda galima naudoti tik kas 2 minutes!");
	new String[216];

    switch(zInfo[playerid][specialybe])
    {
        case 1: format(String, sizeof(String), "Policijos Departamento darbuotojas {DFC043}%s pasiruoðæs priimti iðkvietimus ! /kviesti", GetPlayerNameEx(playerid));

        case 2: format(String, sizeof(String), "Taksi firmos darbuotojas {DFC043}%s pasiruoðæs priimti iðkvietimus ! /kviesti", GetPlayerNameEx(playerid));

        case 5: format(String, sizeof(String), "Medicinos firmos darbuotojas {DFC043}%s pasiruoðæs priimti iðkvietimus ! /kviesti", GetPlayerNameEx(playerid));

        case 3: format(String, sizeof(String), "Mechanikø firmos darbuotojas {DFC043}%s pasiruoðæs priimti iðkvietimus ! /kviesti", GetPlayerNameEx(playerid));

        case 6: format(String, sizeof(String), "Perveþëjø tarnybos firmos darbuotojas {DFC043}%s pasiruoðæs priimti iðkvietimus ! /kviesti", GetPlayerNameEx(playerid));

        case 13: format(String, sizeof(String), "Kartelio firmos darbuotojas {DFC043}%s pasiruoðæs priimti iðkvietimus ! /kviesti", GetPlayerNameEx(playerid));

        default: return 0;
    }

    SendClientMessageToAll(-1, String);
    zInfo[playerid][DirbuEme] = gettime() + 120;

    return 1;
}
CMD:skelbti(playerid, params[])
{
    new tekstas[256], masyvas[300];
    if(!IsPlayerInRangeOfPoint(playerid,20, 1766.7163,397.2422,2019.1133) /*&& !IsPlayerInRangeOfPoint(playerid,20, -99.0179,1086.4789,19.7422)*/)
	{
	     SendClientMessage(playerid, RED, "{DFC043} {ffffff}Skelbti galima tik skelbimø kontoroje!");
	     return 1;
	}
    if(zInfo[playerid][KadaSkelbe] > gettime()) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Skelbti galima tik kas 5 min.!");
    //if(GetPlayerInterior(playerid) < 8) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs ne skelbimo kontoroje!");
    if(GetPlayerScore(playerid) < 2000) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs neturite 2'000XP");
    if(GetPlayerMoneyA(playerid) < 1000) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs neturite 1.000 ");
    if(sscanf(params, "s[256]",tekstas)) return SendClientMessage(playerid, BLUE, "{DFC043} {ffffff}Skelbti skelbimà: /skelbti [Tekstas]");
    else
    {
        format(masyvas, 300, "{7f8c8d}[SKELBIMAS]: [{95a5a6}%s(%i){7f8c8d}] skelbia: {95a5a6}%s",GetPlayerNameEx(playerid),playerid, tekstas);
        //format(masyvas, 300, "{95a5a6}%s",tekstas);
        SendClientMessageToAll(BLUE, masyvas);
        GivePlayerMoneyA(playerid, -1000);
        zInfo[playerid][KadaSkelbe] = gettime() + 5*60;
    }
    return true;
}
