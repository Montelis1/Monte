stock ClearAntiCheatVariables(playerid)
{
    AntiCheatSystem[playerid][pTextSpam] = 0;
    AntiCheatSystem[playerid][pCommandSpam] = 0;
    AntiCheatSystem[playerid][CanCheckPlayer] = 0;
    AntiCheatSystem[playerid][pCurrentPos][0] = 0.0;
    AntiCheatSystem[playerid][pCurrentPos][1] = 0.0;
    AntiCheatSystem[playerid][pCurrentPos][2] = 0.0;
    AntiCheatSystem[playerid][pWeaponsWarns] = 0;
    AntiCheatSystem[playerid][pNoCheckWeapons] = 0;
    AntiCheatSystem[playerid][pNoCheckAirbreak] = 0;
    AntiCheatSystem[playerid][pLastTickCheck] = 0;
    AntiCheatSystem[playerid][pTextSpam] = 0;
    AntiCheatSystem[playerid][pCommandSpam] = 0;
    AntiCheatSystem[playerid][PlayerShooting] = false;
    AntiCheatSystem[playerid][CurrentWeapon] = 0;
    return 1;
}
stock CheckAntiCheat(playerid)
{
    if(Prisijunges[playerid])
    {
        if(zInfo[playerid][admin] > 3) return 1;
        if(AntiCheatSystem[playerid][CanCheckPlayer] == 1)
        {

            new Float: X, Float: Y, Float: Z; GetPlayerPos(playerid, X, Y, Z);

            if((GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && GetPlayerSpeed(playerid) > 0) || GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
            {
                if(GetPlayerSpeed(playerid) < 30)
                {
                    if(Z < 900.0)
                    {
                        new AirBreaking = (floatround(floatsqroot(GetSquareDistance(X, Y, Z, AntiCheatSystem[playerid][pCurrentPos][0], AntiCheatSystem[playerid][pCurrentPos][1], AntiCheatSystem[playerid][pCurrentPos][2])) * 3600) / (GetTickCount () - AntiCheatSystem[playerid][pLastTickCheck]));
                        if(AirBreaking >= 280 && AirBreaking <= 1000)
                        {
                            if(AntiCheatSystem[playerid][pNoCheckAirbreak] == 0)
                            {
                                if(IsPlayerInWater(playerid) == 0)
                                {
                                    new String[128];
                                    format(String, sizeof(String), "{ffffff}>{ff3300} Þaidëjas ({ffffff}%s{ff3300}) buvo iðmestas ið serverio uþ AIR-BREAK naudojimà!", GetPlayerNameEx(playerid));
                                    SendClientMessageToAll(-1, String);
                                    _Kick(playerid);
                                }
                            }
                            else if(AntiCheatSystem[playerid][pNoCheckAirbreak] > 0)
                            {
                                AntiCheatSystem[playerid][pNoCheckAirbreak] --;
                            }
                        }
                    }
                }
            }
            AntiCheatSystem[playerid][pCurrentPos][0] = X;
            AntiCheatSystem[playerid][pCurrentPos][1] = Y;
            AntiCheatSystem[playerid][pCurrentPos][2] = Z;
            AntiCheatSystem[playerid][pLastTickCheck] = GetTickCount();
        }
        if(IsPlayerNPC(playerid)) return 1;
    }
    return 1;
}
public StartAntiCheat(playerid)
{
    AntiCheatSystem[playerid][CanCheckPlayer] = 1;
    return 1;
}
public GlobalAntiCheatTimer()
{
    for(new playerid = 0; playerid < MAX_PLAYERS; playerid ++)
    {
        if(IsPlayerConnected(playerid))
        {
            if(!IsPlayerNPC(playerid)) { CheckAntiCheat(playerid); } // Atnaujinam Anti-Cheat ;)
        }
    }
    return 1;
}
