stock RemovePlayerWeapon(playerid, weaponid)
{
    new plyWeapons[12] = 0;
    new plyAmmo[12] = 0;
    for(new slot = 3; slot != 12; slot++)
    {
        new wep, ammo;
        GetPlayerWeaponData(playerid, slot, wep, ammo);

        if(wep != weaponid && ammo != 3 && Weapons[playerid][wep] == true)
        {
            GetPlayerWeaponData(playerid, slot, plyWeapons[slot], plyAmmo[slot]);
        }
    }
    ResetPlayerWeaponsEx(playerid);
    for(new slot = 3; slot != 12; slot++)
    {
        if(plyAmmo[slot] != 3)
        {
            GivePlayerWeaponsEx(playerid, plyWeapons[slot], plyAmmo[slot]);
        }
    }
    return 1;
}
