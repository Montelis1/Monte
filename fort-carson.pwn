        if(!PlayerToPoint(5, playerid, poz[0],poz[1],poz[2]))
        {
            SendClientMessage(playerid, RED, "{DFC043} {ffffff}Þaidëjas per toli...");
            return 1;
        }
        if(zInfo[id][cuffed])
        {
            if(PlayerToPoint(70, playerid, -682.5672,955.0754,12.4820))
            {
                zInfo[id][cuffed]=false;
                zInfo[id][suristas]=false;
                TogglePlayerControllable(id,true);
                RemovePlayerAttachedObject(id,0);
                SetPlayerSpecialAction(id,SPECIAL_ACTION_NONE);
                format(masyvas, sizeof(masyvas),"* Iðlaisvinote %s.",vardas);
                SendClientMessage(playerid,Raudona,masyvas);
                format(masyvas, sizeof(masyvas),"Jus iðlaisvino %s.",vardas2);
                SendClientMessage(id,Raudona,masyvas);
                return 1;
            }
            else return SendClientMessage(playerid, -1, "Iðlaisvinti þmogø galite tik savo bazëje.");
        }
        if(zInfo[id][suristas])
        {
            zInfo[id][suristas] = false;
            TogglePlayerControllable(id,true);
            RemovePlayerAttachedObject(id,0);
            SetPlayerSpecialAction(id,SPECIAL_ACTION_NONE);
            format(masyvas, sizeof(masyvas),"* Atriðote %s.",vardas);
            SendClientMessage(playerid,Raudona,masyvas);
            format(masyvas, sizeof(masyvas),"* Jûs atriðo %s",vardas2);
            SendClientMessage(id,Raudona,masyvas);
        }
        else
        {
            zInfo[id][suristas]=true;
            TogglePlayerControllable(id,false);
            SetPlayerSpecialAction(id, SPECIAL_ACTION_CUFFED );
            SetPlayerAttachedObject(id,0,19418,5,0.016000,0.032000,0.025000,17.500005,-10.099991,-48.099990,1.0,1.0,1.0 );
            format(masyvas, sizeof(masyvas),"* Suriðote %s.",vardas);
            SendClientMessage(playerid,Raudona,masyvas);
            format(masyvas, sizeof(masyvas),"* Jus suriðo %s.",vardas2);
            SendClientMessage(id,Raudona,masyvas);
        }
    }
    return 1;
}
