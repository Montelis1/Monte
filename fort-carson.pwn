public SpeedoTimer()
{
    for(new p = 0; p < MAX_PLAYERS; p++)
    {
        if(IsPlayerNPC(p) || !IsPlayerConnected(p) || !IsPlayerInAnyVehicle(p) || vehicleInfo[ p ][ useAttach ]) continue;
        if(GetPlayerState(p) == PLAYER_STATE_DRIVER) tInfo[GetPlayerVehicleID(p)][Rida] += (floatsqroot(GetPlayerSpeed(p)) * 50) / 100000;
        new vehicleid = GetPlayerVehicleID(p);
        if(GetPlayerState(p) == PLAYER_STATE_PASSENGER && GetVehicleModel(vehicleid) == 418)
        {
            TaxiAtstumas[p] += (floatsqroot(GetPlayerSpeed(p)) * 50) / 100000;
            UpdateTaksom(p);
        }
        if(GetPlayerState(p) == PLAYER_STATE_PASSENGER && GetVehicleModel(vehicleid) == 420)
        {
            TaxiAtstumas[p] += (floatsqroot(GetPlayerSpeed(p)) * 50) / 100000;
            UpdateTaksom(p);
        }
        if(GetPlayerState(p) == PLAYER_STATE_PASSENGER && GetVehicleModel(vehicleid) == 559)
        {
            TaxiAtstumas[p] += (floatsqroot(GetPlayerSpeed(p)) * 50) / 100000;
            UpdateTaksom(p);
        }
        if(GetPlayerState(p) == PLAYER_STATE_PASSENGER && vehicleInfo[ vehicleid ][ useAttach ])
        {
            TaxiAtstumas[p] += (floatsqroot(GetPlayerSpeed(p)) * 50) / 100000;
            UpdateTaksom(p);
        }
        UpdateSpeedo(p);
    }
    return 1;
}
