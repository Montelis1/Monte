stock CreateEntrance( name[], Float:entX, Float: entY, Float:entZ, Float:iX, Float:iY, Float:iZ, iInt, eInt )
{
    totalEntrances ++;
    format( entData[ totalEntrances ][ entName ], 55, "%s", name );
    entData[ totalEntrances ][ entXx ] = entX;
    entData[ totalEntrances ][ entYy ] = entY;
    entData[ totalEntrances ][ entZz ] = entZ;
    entData[ totalEntrances ][ iXx ] = iX;
    entData[ totalEntrances ][ iYy ] = iY;
    entData[ totalEntrances ][ iZz ] = iZ;
    entData[ totalEntrances ][ iIntt ] = iInt;
    entData[ totalEntrances ][ eIntt ] = eInt;
    new text[ 125 ];
    format(text, sizeof text, "{DFC043}%s\n{FFFFFF}Norëdami áeiti spauskite {DFC043}ENTER", name);
    Create3DTextLabel(text, -1, entX, entY, entZ, 15.0, 0, 0);
    Create3DTextLabel("{DFC043}Iðëjimas\n{FFFFFF}Norëdami iðeiti spauskite{DFC043} ENTER", -1, iX, iY, iZ, 15.0, 0, 0);
    CreatePickupA(19197, 1, entX, entY, entZ);
    CreatePickupA(19197, 1, iX, iY, iZ);
    return totalEntrances;
}

