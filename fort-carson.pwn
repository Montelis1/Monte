CMD:ginklas(playerid, params[])
{
	if(zInfo[playerid][gaujosID] == 0) return 0;
    new giveplayerid, AmmoCount, Priceg;
    if(GetPlayerScore(playerid) < 1500) return SendClientMessage(playerid, RED, "{DFC043} {ffffff}Jûs per maþai patyræs!");
    if(!sscanf(params, "uii", giveplayerid, AmmoCount, Priceg))
    {
            if(Priceg <= 0) return SendClientMessage(playerid, -1, "Kaina turi bûti didesnë nei 0 .");
            if(Priceg >= 5000) return SendClientMessage(playerid, -1, "Kaina ne daugiau kaip 5000");
            if(GetPlayerScore(giveplayerid) < 1500)
            {
                return SendClientMessage(playerid, -1, "{DFC043} {ffffff}Þaidëjas dar naujokas, todël jam parduoti negalite!");
            }
            if(IsPlayerInAnyVehicle(playerid)) SendClientMessage(playerid, -1, "{DFC043} {ffffff}Ðios komandos maðinoje naudoti negalite.");
            if(giveplayerid != INVALID_PLAYER_ID)
            {
                new Float: X, Float: Y, Float: Z; GetPlayerPos(giveplayerid, X, Y, Z);
                if(!IsPlayerInRangeOfPoint(playerid, 10.0, X, Y, Z))
                {
                    SendClientMessage(playerid, -1, "{DFC043} {ffffff}Þaidëjas per toli!");
                    return 1;
                }
                if(giveplayerid == playerid)
                {
                    SendClientMessage(playerid, -1, "{DFC043} {ffffff}Sau ginklø duoti negalite!");
                    return 1;
                }
                if(GetPlayerAmmo(playerid) < AmmoCount)
                {
                    SendClientMessage(playerid, -1, "{DFC043} {ffffff}Neturi tiek ðoviniø!");
                    return 1;
                }

                switch(GetPlayerWeapon(playerid))
                {
                    case 9, 18, 35..40, 44, 45:
                    {
                        ResetPlayerWeaponsEx(playerid);

                        _Kick(playerid);
                    }
                }

                PardavejasKaina[giveplayerid] = Priceg;
                PardavejasAMMO[giveplayerid] = AmmoCount;
                PardavejasGUN[giveplayerid] = GetPlayerWeapon(playerid);
                PardavejasID[giveplayerid] = playerid;

                new String[512], WeaponName[64];
                GetWeaponName(GetPlayerWeapon(playerid), WeaponName, sizeof(WeaponName));

                format(String, sizeof String, "{FF9900}Ar norite pirkti {33CC00}%s {FF9900}jums duoda pasiulyma:\n \n{FFFFFF}%d kulkas uþ vienos kulkos kaina %i \nAr priimate ðá pasiûlymà?", WeaponName, AmmoCount, Priceg);
                ShowPlayerDialogEx(giveplayerid, GunSellDialog, DIALOG_STYLE_MSGBOX, "{FFFFFF}Ginklø sandoris", String, "Pirkti", "Nepirkti");

                format(String, sizeof String, "{FFFFFF}* {33AA33}Pasiûlëte %s pirkti %s %d kulkø uþ %i .", GetPlayerNameEx(giveplayerid), WeaponName, AmmoCount, Priceg*AmmoCount);
                SendClientMessage(playerid, -1, String);
            }
            else
            {
                SendClientMessage(playerid, -1, "{DFC043} {ffffff}þaidëjas neprisijungæs.");
            }
    }
    else
    {
        SendClientMessage(playerid, -1, "{F2F2F2}Naudojimas: /ginklas [þaidëjo Vardas/þaidëjo ID] [Kulkø skaièius] [Vienos kulkos kaina]");
        SendClientMessage(playerid, -1, "{F2F2F2}Funkcija: Pasiûlysite pirkti ginkla.");
    }
    return 1;
}
