{
	if(IsPlayerInRangeOfPoint(playerid, 5.0, playerAFK[playerid][0], playerAFK[playerid][1], playerAFK[playerid][2]) && playerAFK[playerid][0] != 0 && playerAFK[playerid][1] != 0)
