stock KMAVehicleEx(vehicleid, trailerid, Float:kX, Float:kY, Float:kZ)
{
	new Float: bx, Float: by;
	GetVehicleTrailer(vehicleid);
	SetVehiclePos(vehicleid, kX, kY, kZ);
	GetXYBehindVehicle(vehicleid, bx, by, 5.0);
	SetVehiclePos(trailerid, bx, by, kZ);
	AttachTrailerToVehicle(trailerid, vehicleid);
	return true;
}

stock GetXYBehindVehicle(vehicleid, &Float:X, &Float:Y, Float:distance)
{
	new Float: A;
	GetVehiclePos(vehicleid, X, Y, A);
	GetVehicleZAngle(vehicleid, A);
	X += ( distance * floatsin( -A+180, degrees ));
	Y += ( distance * floatcos( -A+180, degrees ));
}

stock Teleportas(playerid, Float:Xx, Float:Yy, Float:Zz)
{
    new vehicleid = GetPlayerVehicleID(playerid);
    if(vehicleid)
    {
        //if(GetPlayerVehicleSeat(playerid) > 0) return 0;
        SetVehiclePos(vehicleid, Xx, Yy, Zz);
        new Float: angle;
        GetVehicleZAngle(vehicleid, angle);
        SetVehicleZAngle(vehicleid, angle);
    }
    else
    {
        SetPlayerPos(playerid, Xx, Yy, Zz);
    }
    return 1;
}

